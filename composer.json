{
    "name": "ufukcanatann/prism",
    "description": "PRISM Framework - Modern PHP Framework for Enterprise Applications",
    "type": "project",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "framework",
        "php",
        "api",
        "enterprise",
        "modern"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "PRISM Framework Team",
            "email": "team@prism-framework.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/http-foundation": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/console": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/cache": "^6.0",
        "symfony/config": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/expression-language": "^6.0",
        "filp/whoops": "^2.15",
        "vlucas/phpdotenv": "^5.5",
        "illuminate/view": "^10.0",
        "jenssegers/blade": "^2.0",
        "paragonie/anti-csrf": "^2.4",
        "defuse/php-encryption": "^2.3",
        "monolog/monolog": "^3.0",
        "league/container": "^4.0",
        "league/event": "^2.0",
        "league/route": "^5.0",
        "league/plates": "^3.5",
        "php-di/php-di": "^7.0",
        "nikic/fast-route": "^1.3",
        "doctrine/annotations": "^2.0",
        "doctrine/cache": "^2.0",
        "doctrine/collections": "^1.8",
        "doctrine/inflector": "^2.0",
        "doctrine/lexer": "^2.0",
        "doctrine/orm": "^2.11",
        "doctrine/dbal": "^3.0",
        "doctrine/migrations": "^3.0",
        "ramsey/uuid": "^4.0",
        "ramsey/collection": "^1.2",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^3.0",
        "psr/cache": "^3.0",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "fakerphp/faker": "^1.20"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        },
        "files": [
            "core/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php core/install.php"
        ],
        "post-update-cmd": [
            "php core/install.php"
        ],
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "prism": "php prism"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ufukcanatann/prism"
        }
    ]
}